programa fibonnaciFor;

%% Fibonnaci with for loop
principal() {
    variables entero : i,n,t1,t2,nxt;
    n = 9;
    nxt = 0;
    t1 = 0;
    t2 = 1;
    escribe("Fibonnaci Serie");
    desde i = 1; hasta i < n; hacer i = i + 1; {
        si (i == 0) entonces {
            escribe(t1);
        }
        si(i == 2) entonces {
            escribe(t2);
        }
        nxt = t1 + t2;
        t1 = t2;
        t2 = nxt;
        escribe(nxt);
    }
}